<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<CLIP CATEGORY="Motor Control" FILE="Arduino-STM32/Arduino Motor Shield with DC Motors.pdsclip" DESC="Arduino Motor Shield (R3) with DC Motors" FAMILY="Arduino-STM32">
   <PERIPHERAL TYPE="DC Motor 1" ELEMENT="M1">
      <INTERFACE OBJECT="DCMotorV1">
          <PIN NAME="dir" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA2"/>
          </PIN> 
          <PIN NAME="pwm" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA0"/>
          </PIN> 
          <PIN NAME="brake" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA3"/>
          </PIN> 
         <SETUP NAME="begin"/>
      </INTERFACE>

      <!-- Firmware Driver -->
      <DRIVER FILE="motor_shield_v1/DC Motor.xml"/>
      
   </PERIPHERAL>

   <PERIPHERAL TYPE="DC Motor 2" ELEMENT="M2">
      <INTERFACE OBJECT="DCMotorV1">
          <PIN NAME="dir" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA4"/>
          </PIN> 
          <PIN NAME="pwm" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA1"/>
          </PIN> 
          <PIN NAME="brake" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA5"/>
          </PIN> 
         <SETUP NAME="begin"/>
      </INTERFACE>

      <!-- Firmware Driver -->
      <DRIVER FILE="motor_shield_v1/DC Motor.xml"/>
      
   </PERIPHERAL>

</CLIP>


