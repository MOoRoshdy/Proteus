<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<CLIP CATEGORY="Motor Control" FILE="Arduino-STM32/Arduino Motor Shield with Stepper Motor.pdsclip" DESC="Arduino Motor Shield (R3) with Stepper Motor" FAMILY="Arduino-STM32">
   <PERIPHERAL TYPE="Stepper Motor" ELEMENT="M1">
      <INTERFACE OBJECT="StepperMotorV1">
          <PIN NAME="dir1" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA10"/>
          </PIN> 
          <PIN NAME="pwm1" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA8"/>
          </PIN> 
          <PIN NAME="brake1" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA11"/>
          </PIN> 
          <PIN NAME="dir2" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA12"/>
          </PIN> 
          <PIN NAME="pwm2" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA9"/>
          </PIN> 
          <PIN NAME="brake2" TYPE="DIGITAL">
              <TERMINAL ELEMENT="PA15"/>
          </PIN> 
          <PROPERTY NAME="STEP" ELEMENT="M1" TYPE="INTEGER"/>
         <SETUP NAME="begin"/>
      </INTERFACE>

      <!-- Firmware Driver -->
      <DRIVER FILE="motor_shield_v1/Stepper Motor.xml"/>
      
   </PERIPHERAL>

   
</CLIP>


