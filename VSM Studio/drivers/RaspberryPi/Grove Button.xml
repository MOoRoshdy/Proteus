<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<CLIP CATEGORY="Grove" FILE="Grove/Grove Button.pdsclip" FAMILY="Raspberry Pi" DESC="Momentary Action Push Button">
   <PERIPHERAL TYPE="Grove Button" ELEMENT="BTN1" >
     <!-- Hardware Interface -->
     <INTERFACE OBJECT="GroveButton">
      <CONFIG>
       <PROPERTY NAME="STATE" ALIAS="PORT" ELEMENT="GC1" TYPE="INTEGER" GROVE="DIGITAL"/>
      </CONFIG>
     </INTERFACE>
   
     <!-- Firmware Driver -->
     <DRIVER HEADER="Grove">
       <REQUIRE MODULE="GROVE"/>
        <FUNCTION DESC="Button State"> 
          <RETURN TYPE="BOOLEAN"/>
        </FUNCTION>
     </DRIVER>
   </PERIPHERAL>
</CLIP>	
  